<UserControl x:Class="Site.Pages.DashBoard.DashBoard"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Site.Pages.DashBoard"
             xmlns:resx="clr-namespace:Site.Globalizacion"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
>

    <Grid>
        <StackPanel>
            <Grid Height="Auto">
                <Label VerticalAlignment="Center" FontSize="24" FontWeight="Bold" Background="{x:Null}" BorderBrush="{x:Null}"
                       Content="{x:Static resx:DashBoardPageResource.lblTituloDashBoard}"  />
            </Grid>
        </StackPanel>
    </Grid>
</UserControl>
